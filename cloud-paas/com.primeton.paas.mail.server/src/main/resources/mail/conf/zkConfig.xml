<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config xmlns="http://org.gocom.cloud/xmlns/cesium/1.0">
	<module name="zkServersConfig">
		<!-- default zookeeper server config -->
		<group name="default">
			<!-- zkServers connection string -->
			<configValue key="zkServers">192.168.100.211:2181,192.168.100.212:2182,192.168.100.213:2183</configValue>
			<!-- zooKeeper timeout ,unit million seconds -->
			<configValue key="connectionTimeout">60000</configValue>
		</group>
	</module>
</config>